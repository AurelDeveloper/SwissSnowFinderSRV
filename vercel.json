{
    "rewrites": [
        { "source": "/(.*)", "destination": "/api/index" }
    ],
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": { "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/api/index", "dest": "/api/index.py"
        }
    ],
    "crons": [
        {
            "path": "/api/index",
            "schedule": "0 0 * * *"
        }
    ]
}